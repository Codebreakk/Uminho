   "source": [
    "def pmenosum(n):\n",
    "    Zn = IntegerModRing(n)\n",
    "    r = Zn(2)\n",
    "    k = 1\n",
    "    while gcd(r-1,n) == 1:\n",
    "        k = k + 1\n",
    "        r = r^k\n",
    "    return gcd(r-1, n)"
   ]
